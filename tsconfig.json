{
  "compilerOptions": {
    "target": "es2022",
    "strict": true,
    "skipLibCheck": true,
    "skipDefaultLibCheck": true,
    "noImplicitAny": true,
    "moduleResolution": "nodenext",
    "module": "NodeNext",
    "lib": ["es2023"],
    "incremental": true,
    "forceConsistentCasingInFileNames": true,
    "esModuleInterop": true,
    "allowJs": false
  }
}
